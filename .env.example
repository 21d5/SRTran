# Google AI Studio Configuration (Preferred)
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_AI_MODEL=gemini-2.0-flash-exp  # Optional, defaults to gemini-2.0-flash-exp
GOOGLE_AI_RPM=60  # Optional, defaults to 0 (no limit). Free tier default is 60

# OpenRouter Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet  # Optional, defaults to anthropic/claude-3.5-sonnet
OPENROUTER_RPM=50  # Optional, defaults to 0 (no limit). Check your plan's limits

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4  # Optional, defaults to gpt-4
OPENAI_RPM=200  # Optional, defaults to 0 (no limit). GPT-4 default is 200, GPT-3.5 is 3500

# Uncomment and set to true to enable verbose output
# VERBOSE=true
